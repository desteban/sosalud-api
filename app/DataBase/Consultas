SELECT * FROM tmp_AH
LEFT JOIN tmp_us ON tmp_us.tipoIdentificacion=tmp_AH.tipoIdentificacion and tmp_us.identificacion=tmp_AH.identificacion
WHERE tmp_us.identificacion is NULL;

SELECT * FROM tmp_AH 
LEFT JOIN tmp_af using(codigoIps,numeroFactura)
WHERE !(tmp_af.fechaInicio <= tmp_AH.fechaingreso and tmp_AH.fechaingreso <= tmp_af.fechaFinal);


/* VALIDACION DEL ARCHIVO DE RIPS AM */

