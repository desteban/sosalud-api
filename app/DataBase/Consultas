/* VALIDACION DEL ARCHIVO DE RIPS AP */

SELECT * FROM tmp_AP 
LEFT JOIN tmp_af ON tmp_af.codigoIps = tmp_AP.codigoIps and tmp_af.numeroFactura=tmp_AP.numeroFactura
WHERE tmp_af.codigoIps is NULL;

SELECT * FROM tmp_AP
LEFT JOIN tmp_us ON tmp_us.tipoIdentificacion=tmp_AP.tipoIdentificacion and tmp_us.identificacion=tmp_AP.identificacion
WHERE tmp_us.identificacion is NULL;

SELECT * FROM tmp_AP 
LEFT JOIN tmp_af using(codigoIps,numeroFactura)
WHERE !(tmp_af.fechaInicio <= tmp_AP.fechaProcedimiento and tmp_AP.fechaProcedimiento <= tmp_af.fechaFinal);


/* VALIDACION DEL ARCHIVO DE RIPS AC */

SELECT * FROM tmp_AC
LEFT JOIN tmp_af ON tmp_af.codigoIps = tmp_AC.codigoIps and tmp_af.numeroFactura=tmp_AC.numeroFactura
WHERE tmp_af.codigoIps is NULL;

SELECT * FROM tmp_AC
LEFT JOIN tmp_us ON tmp_us.tipoIdentificacion=tmp_AC.tipoIdentificacion and tmp_us.identificacion=tmp_AC.identificacion
WHERE tmp_us.identificacion is NULL;

SELECT * FROM tmp_AC 
LEFT JOIN tmp_af using(codigoIps,numeroFactura)
WHERE !(tmp_af.fechaInicio <= tmp_AC.fechaConsulta and tmp_AC.fechaConsulta <= tmp_af.fechaFinal);


/* VALIDACION DEL ARCHIVO DE RIPS AH */

SELECT * FROM tmp_AH 
